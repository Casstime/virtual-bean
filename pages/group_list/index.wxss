/* pages/group_list/index.wxss */
page {
    background-color: #ececec;
}
.group-item {
    display: flex;
    justify-content: space-between;
    padding: 0 15px 0 20px;
    margin-top: 20px;
    height: 50px;
    background-color: #fff;
    line-height: 50px;
}
.group-item:first-child {
    margin-top: 0;
}
image {
    position: relative;
    top: 15px;
    width: 20px;
    height: 20px;
}